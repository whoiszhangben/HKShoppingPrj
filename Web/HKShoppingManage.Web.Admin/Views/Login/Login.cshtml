@using System.Web.Optimization
@{
    Layout = null;
}


<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>HK代购管理平台</title>
    <link href="~/Content/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    @Styles.Render("~/resource/common_css", "~/resource/login_css")
    <script src="~/Scripts/jquery-1.10.2.min.js" type="text/javascript"></script>
    @Scripts.Render("~/resource/login_js")
</head>
<body class="whiteSkin">
    <div class="loginTop"></div>
    <ul class="loginMid">
        <li>
            <i class="icons user"></i>
            <input id="LoginCode" type="text" class="IsRequired" maxlength="20" value="" placeholder="用户名" />
        </li>
        <li>
            <i class="icons password"></i>
            <input id="LoginPwd" type="password" class="IsRequired" maxlength="20" value="" placeholder="密码" />
        </li>
        <li class="coder">
            <input type="text" id="ValidateCode" style="padding-left:5px;">
            <img id="yzmImg" width="100" height="32" style="cursor:pointer">
            <a id="hyz" href="javascript:void(0)">看不清，换一张</a>
        </li>
        <div class="mt16 win100 fl" style="padding:0 150px;">
            <input id="btnLogin" type="button" value="登录">
        </div>
    </ul>
    <div class="loginBottom">深圳市多元世纪对外行情服务管理平台V1.0</div>
    <div class="erroBox"></div>
    <div class="progressBar">正在操作，请稍候...</div>
    <div class="windows-mark"></div>
</body>
</html>
